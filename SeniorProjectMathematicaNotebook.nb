Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GoldenChainBasis", "[", "1", "]"}], " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"2", ",", "2"}], "}"}], "}"}]}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GoldenChainBasis", "[", "2", "]"}], " ", "=", " ", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"1", ",", "2", ",", "1"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"2", ",", "1", ",", "2"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"2", ",", "2", ",", "2"}], "}"}]}], 
   "}"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GoldenChainBasis", "[", "L_", "]"}], ":=", 
  RowBox[{
   RowBox[{"GoldenChainBasis", "[", "L", "]"}], "=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"PrevChain", "=", 
        RowBox[{"GoldenChainBasis", "[", 
         RowBox[{"L", "-", "1"}], "]"}]}], ",", 
       RowBox[{"SecondPrevChain", "=", " ", 
        RowBox[{"GoldenChainBasis", "[", 
         RowBox[{"L", "-", "2"}], "]"}]}], ",", 
       RowBox[{"GoldenTemp", "=", 
        RowBox[{"{", "}"}]}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"PrevChain", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}], "\[Equal]", "2"}], "&&", 
           RowBox[{
            RowBox[{
             RowBox[{"PrevChain", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "2"}]}], ",", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"GoldenTemp", ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Insert", "[", 
               RowBox[{
                RowBox[{"PrevChain", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", "2", ",", " ", "2"}], 
               "]"}], ",", 
              RowBox[{"Insert", "[", 
               RowBox[{
                RowBox[{"PrevChain", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", "1", ",", "2"}], 
               "]"}]}], "}"}]}], "]"}], ",", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"GoldenTemp", ",", 
            RowBox[{"{", 
             RowBox[{"Insert", "[", 
              RowBox[{
               RowBox[{"PrevChain", "[", 
                RowBox[{"[", "i", "]"}], "]"}], ",", "2", ",", " ", "2"}], 
              "]"}], "}"}]}], "]"}], ",", 
          RowBox[{"Return", "[", "\"\<Something went wrong\>\"", "]"}]}], 
         "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "PrevChain", "]"}]}], "}"}]}], "]"}], ";", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"SecondPrevChain", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "[", 
            RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "1"}], ",", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"GoldenTemp", ",", 
            RowBox[{"{", 
             RowBox[{"Flatten", "[", 
              RowBox[{
               RowBox[{"PrependTo", "[", 
                RowBox[{
                 RowBox[{"SecondPrevChain", "[", 
                  RowBox[{"[", "i", "]"}], "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"1", ",", " ", "2"}], "}"}]}], "]"}], ",", "1"}], 
              "]"}], "}"}]}], "]"}], ",", 
          RowBox[{"Continue", "[", "]"}], ",", 
          RowBox[{"Return", "[", "\"\<Something went wrong\>\"", "]"}]}], 
         "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "SecondPrevChain", "]"}]}], "}"}]}], "]"}], 
      ";", 
      RowBox[{"Sort", "[", 
       RowBox[{"Flatten", "[", 
        RowBox[{"GoldenTemp", ",", "1"}], "]"}], "]"}]}]}], 
    "]"}]}]}]}], "Input",
